{
  "name": "fsh-ihsus",
  "version": "0.0.1",
  "description": "Ichthymorphic Helper Systematically Undoes Sushi (FSH Decompiler)",
  "main": "dist/app.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "check": "npm run test && npm run lint && npm run prettier",
    "test": "jest --maxWorkers=4 --coverage",
    "test:watch": "npm run test -- --watchAll",
    "coverage": "opener coverage/lcov-report/index.html",
    "lint": "tsc && eslint \"**/*.{js,ts}\"",
    "lint:fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier --check \"**/*.{js,ts}\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts}\""
  },
  "bin": {
    "ihsus": "dist/app.js"
  },
  "types": "dist/app.d.ts",
  "files": [
    "dist/**/*.{js,json,d.ts}"
  ],
  "author": "",
  "contributors": [
    "Mint Thompson <mathompson@mitre.org>"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.34",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "jest": "^25.2.4",
    "jest-extended": "^0.11.5",
    "opener": "^1.5.1",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "commander": "^5.1.0",
    "fs-extra": "^9.0.1",
    "fsh-sushi": "^0.13.2"
  }
}
